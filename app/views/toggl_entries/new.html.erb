<h2><%= l('toggl.toggl_entry') %></h2>

<%= labelled_form_for @toggl_entry, :url => toggl_entries_path do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_tag l(:button_create) %>
  <%= submit_tag l(:button_create_and_continue), :name => 'continue' %>
  <%= link_to l(:button_cancel), toggl_entries_path %>
<% end %>
